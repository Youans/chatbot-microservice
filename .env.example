# Example environment variables for local docker compose
# Copy to .env and adjust as needed

# Gateway JWT
GATEWAY_JWT_SECRET=dev-secret-please-change
GATEWAY_JWT_ISSUER=http://pidima.local
GATEWAY_JWT_AUDIENCE=chatbot

# Chatbot persistence and LLM
# Options: dynamodb | memory
APP_PERSISTENCE=dynamodb
APP_LLM_PROVIDER=dummy
APP_LLM_MODEL=dummy-model
# Required when using app.llm-provider=openai
OPENAI_API_KEY=
